{
  "url": "http://localhost:3030/",
  "host": "localhost",
  "port": 3030,
  "public": "../public/",
  "admin": "../admin/dist/",
  "uploads": "../uploads/",
  "paginate": {
    "default": 10,
    "max": 50
  },
  "authentication": {
    "secret": "",
    "strategies": [
      "jwt",
      "local"
    ],
    "path": "/authentication",
    "service": "users",
    "jwt": {
      "header": {
        "typ": "access"
      },
      "audience": "https://yourdomain.com",
      "subject": "anonymous",
      "issuer": "feathers",
      "algorithm": "HS256",
      "expiresIn": "1d"
    },
    "local": {
      "entity": "user",
      "usernameField": "email",
      "passwordField": "password"
    }
  },
  "postgres": "postgres://sysadmin:sysadmin@localhost:5432/sysadmin"
}
